{
  "name": "fullstack-comfy",
  "description": "Fullstack boilerplate with Grunt + Backbone + Express",
  "version": "1.0.0",
  "main": "src/index.js",
  "author": "Thomas Klinger <teekaay@github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/teekaay/fullstack-comfy"
  },
  "keywords": [
