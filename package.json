    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^7.3.0",
    "chai": "^4.1.0",
    "cssnano": "^3.10.0",
    "envify": "^4.1.0",
    "eslint": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-browserify": "^5.0.0",
